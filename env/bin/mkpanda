#!/usr/local/Dropbox/dcos-bootstrap/env/bin/python3.5
# EASY-INSTALL-ENTRY-SCRIPT: 'dcos-image==0.1','console_scripts','mkpanda'
__requires__ = 'dcos-image==0.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('dcos-image==0.1', 'console_scripts', 'mkpanda')()
    )
